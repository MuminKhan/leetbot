{
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.formatOnSave": true,
        "editor.wordBasedSuggestions": "off",
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ]
    },
    "black-formatter.args": [
        "--line-length",
        "120"
    ],
    "pylint.args": [
        "--line-length",
        "120"
    ],
    "files.exclude": {
        ".aws-sam": true,
    }
}